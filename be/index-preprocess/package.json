		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"fs": "^0.0.1-security",
		"jimp": "^0.22.10",
		"jsonwebtoken": "^9.0.2",
		"mariadb": "^3.2.1",
		"morgan": "^1.10.0",
		"multer": "^1.4.4",
		"multer-s3": "^3.0.1",
		"pg": "^8.16.3",
		"pino": "^9.11.0",
		"redis": "^4.6.14",
		"sequelize": "^6.33.0",
		"sequelize-cli": "^6.6.1",
		"sockjs": "^0.3.24",
		"sockjs-client": "^1.6.1",
